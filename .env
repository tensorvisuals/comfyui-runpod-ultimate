# Hugging Face Token (required for gated models like FLUX)
# Get from: https://huggingface.co/settings/tokens
HF_TOKEN=

# Civitai API Token (optional, for Civitai downloads)
# Get from: https://civitai.com/user/account
CIVITAI_TOKEN=

# Build Type: minimal, standard, or full
BUILD_TYPE=standard

# Performance Settings
PYTORCH_CUDA_ALLOC_CONF=expandable_segments:True,max_split_size_mb:256
TORCH_ALLOW_TF32_CUBLAS=1
NVIDIA_TF32_OVERRIDE=1
